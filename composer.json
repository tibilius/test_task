{
  "name": "tibilius/paysomething",
  "license": [],
  "type": "project",
  "minimum-stability": "stable",
  "require": {
    "php": "^7.2.0",
    "ext-json": "*",
    "symfony/dependency-injection": "^5.1",
    "guzzlehttp/guzzle": "^7.0",
    "symfony/config": "^5.1",
    "symfony/yaml": "^5.1",
    "monolog/monolog": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "scripts": {
    "post-package-install": ["mkdir -p var/logs var/storage var/test_storage"]
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  }
}
